<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DoradSmartphone.Views.ExerciseDetailsPage"
             xmlns:vm="clr-namespace:DoradSmartphone.ViewModels"
             Title="{Binding Exercise.Date}"
             x:DataType="vm:ExerciseDetailsViewModel">
    <VerticalStackLayout>
        <Label 
            Text="{Binding Exercise.Date}"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
    </VerticalStackLayout>
</ContentPage>